<body class="font-['Open Sans']">
    <form class="bg-[#F0F2FF] flex justify-center items-center h-[100vh] flex-col gap-4" action="/login" method="POST">
        <% if (errorMessage) { %>
            <div class="bg-white border-2 border-[#B7B8B8] w-[350px] rounded-md p-4">
                <p class="text-red-600"><%= errorMessage %></p>
            </div>
        <% } %>
        <div class="bg-white border-2 border-[#B7B8B8] w-[350px] rounded-md p-4">
            <div class="flex flex-col relative">
                <label>UserID :</label>
                <input class="pl-10 pr-4 py-2 rounded-md border-2 border-[#B7B8B8]" type="text" name="user_id" placeholder="UserID" />
                <i class="fa-solid fa-hashtag absolute top-[38px] left-4"></i>
            </div>
            <div class="flex flex-col my-2 relative">
                <label>Password :</label>
                <input class="pl-10 pr-4 py-2 rounded-md border-2 border-[#B7B8B8]" name="password" type="password" placeholder="Password" id="passwordTextBox" />
                <i class="fa-solid fa-lock absolute top-[38px] left-4"></i>
                <button class="absolute text-[#B7B8B8] top-[34px] right-4" onclick="passwordSee()" type="button"><i id="passwordIcon" class="fa-solid fa-eye-slash"></i></button>
            </div>
            <a class="" href="/register">Belum punya akun? <u>klik disini</u></a>
            <button class="bg-black text-white w-full p-2 rounded-md mt-3" type="submit">Login</button>
        </div>
    </form>
